{
  "name": "six-cities-overmind",
  "homepage": "https://nataliailina.github.io/six-cities-overmind",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "start": "concurrently \"overmind-devtools\" \"cross-env NODE_ENV=development webpack-dev-server\"",
    "eslint": "eslint src/**/*.js src/**/*.jsx",
    "test": "npm run eslint && jest",
    "updateSnapshot": "npm test -- -u",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.19",
    "axios-mock-adapter": "1.16.0",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "eslint": "5.13.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "jest": "24.7.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "2.0.5",
    "react-test-renderer": "16.8.6",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "25.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "utility-types": "3.10.0",
    "webpack": "4.30.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.3.1"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "axios": "0.19.2",
    "concurrently": "^5.2.0",
    "leaflet": "1.5.1",
    "overmind": "^23.0.1",
    "overmind-devtools": "^24.0.1",
    "overmind-react": "^24.0.1",
    "page": "^1.11.6",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test-setup.js"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    }
  }
}
